@{
  Layout = "_Layout";
}

<div class="container">

<h1>Here is the list of all specialties!</h1>

@if (@Model.Count == 0)
{
  <h3>No specialties have been added yet!</h3>
}

@foreach (var specialty in Model)
{
  <h3><a href='/specialties/@specialty.GetId()'>@specialty.GetName()</a></h3>
}


 <h3><a href='/specialties/new'>Add a new specialty</a></h3>


</div>